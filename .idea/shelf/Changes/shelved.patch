<+>\r\n\r\n/* Função recursiva para calcular o fatorial de um número */\r\nlong fatr(long n) {\r\n    if (n <= 1)\r\n        return 1;\r\n    else\r\n        return (n * fatr(n - 1));\r\n}\r\n\r\n/* Função com iterações 'for' para calcular o fatorial de um número */\r\nlong fat(long n) {\r\n    long r = 1;\r\n\r\n    for (int i = 1; i <= n; i++)\r\n        r *= i;\r\n\r\n    return r;\r\n}\r\nint somaElementosVetorInt(int v[], int n) {\r\n    if (n == 0) {\r\n        return v[0];\r\n    } else {\r\n        return v[n] + somaElementosVetorInt(v, n - 1);\r\n    }\r\n}
